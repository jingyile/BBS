<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE validators PUBLIC 
		"-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
		"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="itemName">								<!--指定校验itemName属性 -->
		<field-validator type="requiredstring">			<!-- 使用必填字符串校验器 -->
			<param name="trim">true</param>				<!-- 截断两段空白-->
			<message>栏目名称必须输入</message>				<!-- 校验错误信息 -->
		</field-validator>
	</field>
	<field name="manager">								<!--指定校验manager属性 -->
		<field-validator type="requiredstring">			<!-- 使用必填字符串校验器 -->
			<param name="trim">true</param>				<!-- 截断两段空白-->
			<message>版主必须输入</message>				<!-- 校验错误信息 -->
		</field-validator>
	</field>
</validators>

